{
  "name": "littleprak",
  "displayName": "LittlePrak",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "main": "src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "littlePrak.startDalai",
        "title": "Start LittlePrak LLAMA Server"
      },
      {
        "command": "littlePrak.stopDalai",
        "title": "Stop LittlePrak LLAMA Server"
      },
      {
        "command": "littlePrak.generateSuggestions",
        "title": "Generate Suggestions"
      }
    ],
    "keybindings": [
      {
        "command": "littlePrak.commentToCode",
        "key": "ctrl+alt+c",
        "mac": "cmd+alt+c",
        "when": "editorTextFocus && !editorReadonly"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "make": "vsce package",
    "deploy": "vsce publish"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.82.0",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "socket.io-client": "^4.7.2"
  }
}